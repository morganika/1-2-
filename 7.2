#include "pch.h"
#include <iostream>
using namespace std;

class Point{

private:
	int x;
	int y;

public:

	Point()
	{
		x = 0;
		y = 0;
		cout << this << "    constructor" << endl;
	}
	Point(int valueX, int valueY) 
	{
		x = valueX;
		y = valueY;
		cout << this << "    constructor" << endl;
	}
	bool operator ==(const Point & other)
	{
		if (this->x == other.x && this->y == other.y)
		{
			return true;
		}
		else
		{
			return false;
		}
	}
		
};

